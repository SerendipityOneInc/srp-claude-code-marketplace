{
  "mcpServers": {
    "lark": {
      "command": "npx",
      "args": [
        "-y",
        "@larksuiteoapi/lark-mcp",
        "mcp",
        "-a",
        "${LARK_APP_ID}",
        "-s",
        "${LARK_APP_SECRET}",
        "--oauth",
        "--token-mode",
        "auto"
      ],
      "env": {}
    },
    "rube": {
      "type": "http",
      "url": "https://rube.app/mcp",
      "headers": {
        "Authorization": "Bearer ${RUBE_API_KEY}"
      }
    }
  }
}
